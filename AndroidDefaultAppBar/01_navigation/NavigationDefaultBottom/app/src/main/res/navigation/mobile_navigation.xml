<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/mobile_navigation"
            app:startDestination="@+id/nav_menu_rotate">

    <fragment
            android:id="@+id/nav_menu_rotate"
            android:name="com.haoyudu.navigationdefaultbottom.ui.home.HomeFragment"
            android:label="@string/title_home"
            tools:layout="@layout/fragment_home"/>

    <fragment
            android:id="@+id/nav_menu_zoom"
            android:name="com.haoyudu.navigationdefaultbottom.ui.notifications.NotificationsFragment"
            android:label="@string/title_notifications"
            tools:layout="@layout/fragment_notifications"/>

    <fragment
            android:id="@+id/nav_menu_move"
            android:name="com.haoyudu.navigationdefaultbottom.ui.dashboard.DashboardFragment"
            android:label="@string/title_dashboard"
            tools:layout="@layout/fragment_dashboard"/>
</navigation>